<mah:MetroWindow x:Class="Janus_Client_V1.MainWindow"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
                 mc:Ignorable="d"
                 SaveWindowPosition="True"
                 FontFamily="Oswald"
                 x:Name="Hauptfenster"
                  TitleBarHeight="50"
                 ShowIconOnTitleBar="True"
                 AllowsTransparency="True"
                 Opacity="{Binding BG_OPACITY}"
                 Title="Projekt-Janus Client V2" TitleAlignment="Center" Height="700" Width="1200" ResizeMode="NoResize" Icon="apple-icon-72x72.png" WindowStartupLocation="CenterScreen" MaxWidth="1200" MaxHeight="700" Loaded="Hauptfenster_Loaded" Closed="Hauptfenster_Closed" Closing="Hauptfenster_Closing">

    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="job_angenommen" />
        <BooleanToVisibilityConverter x:Key="blinker_links" />
        <BooleanToVisibilityConverter x:Key="blinker_rechts" />
        <BooleanToVisibilityConverter x:Key="elektrik_an" />
        <BooleanToVisibilityConverter x:Key="motor_an" />
        <BooleanToVisibilityConverter x:Key="licht_low" />
        <BooleanToVisibilityConverter x:Key="fernlicht" />
        <BooleanToVisibilityConverter x:Key="standlicht" />
        <BooleanToVisibilityConverter x:Key="bremslicht"/>
        <BooleanToVisibilityConverter x:Key="tank_warnung" />
        <BooleanToVisibilityConverter x:Key="adblue_warnung" />
        <BooleanToVisibilityConverter x:Key="air_warnung" />
        <BooleanToVisibilityConverter x:Key="oil_warnung" />
        <BooleanToVisibilityConverter x:Key="water_warnung" />
        <BooleanToVisibilityConverter x:Key="battery_warnung" />
        <BooleanToVisibilityConverter x:Key="parking_brake" />
        <BooleanToVisibilityConverter x:Key="trailer_attached" />
        <BooleanToVisibilityConverter x:Key="SDK_Aktiv" />
        <BooleanToVisibilityConverter x:Key="SDK_Aktiv2" />

        <ContextMenu x:Key="contextMenu" MenuItem.Click="ContextMenu_Click">
            <MenuItem Header="Item 1"/>
        </ContextMenu>


    </Window.Resources>



    <mah:MetroWindow.Flyouts>
        <mah:FlyoutsControl>
            <mah:Flyout x:Name="LeftFlyOut" Width="270">

                <ScrollViewer Name="scrlMain"
                    CanContentScroll="True"
                    HorizontalScrollBarVisibility="Auto"
                    VerticalScrollBarVisibility="Auto"
                    VerticalAlignment="Stretch">
                    <StackPanel Orientation="Vertical">

                        <Image x:Name="beta_tester_icon" Width="150" Source="Resources\BETA_CLIENT.jpg"/>

                        <Label Content="Farbschema wählen:" HorizontalAlignment="Center"/>
                        <ComboBox Width="150" HorizontalContentAlignment="Center" Padding="10" x:Name="Farbschema" SelectedValuePath="Tag" SelectionChanged="Farbschema_SelectionChanged" Margin="0,0,0,20">
                            <ComboBoxItem Tag="Dark.Red">Dunkel Rot</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Green">Dunkel Grün</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Blue">Dunkel Blau</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Purple">Dunkel PurPur</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Orange">Dunkel Orange</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Lime">Dunkel Limette</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Emerald">Dunkel Emerald</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Teal">Dunkel Teal</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Cyan">Dunkel Cyan</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Cobalt">Dunkel Cobald</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Indigo">Dunkel Indigo</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Violet">Dunkel Violett</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Pink">Dunkel Pink</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Magenta">Dunkel Magenta</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Crimson">Dunkel Crimson</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Amber">Dunkel Amber</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Yellow">Dunkel Gelb</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Brown">Dunkel Braun</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Olive">Dunkel Olive</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Steel">Dunkel Stahl</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Mauve">Dunkel Mauve</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Taupe">Dunkel Taupe</ComboBoxItem>
                            <ComboBoxItem Tag="Dark.Sienna">Dunkel Wüste</ComboBoxItem>

                            <ComboBoxItem Tag="Light.Red">Hell Rot</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Green">Hell Grün</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Blue">Hell Blau</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Purple">Hell PurPur</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Orange">Hell Orange</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Lime">Hell Limette</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Emerald">Hell Emerald</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Teal">Hell Teal</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Cyan">Hell Cyan</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Cobalt">Hell Cobald</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Indigo">Hell Indigo</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Violet">Hell Violett</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Pink">Hell Pink</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Magenta">Hell Magenta</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Crimson">Hell Crimson</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Amber">Hell Amber</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Yellow">Hell Gelb</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Brown">Hell Braun</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Olive">Hell Olive</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Steel">Hell Stahl</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Mauve">Hell Mauve</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Taupe">Hell Taupe</ComboBoxItem>
                            <ComboBoxItem Tag="Light.Sienna">Hell Wüste</ComboBoxItem>
                        </ComboBox>


                        <Label Content="System-Sounds:" HorizontalAlignment="Center"/>
                        <ComboBox Width="150" HorizontalContentAlignment="Center" Padding="10" x:Name="Systemsounds" SelectedValuePath="Tag" SelectionChanged="Systemsounds_SelectionChanged" Margin="0,0,0,20">
                            <ComboBoxItem Tag="An">An</ComboBoxItem>
                            <ComboBoxItem Tag="Aus">Aus</ComboBoxItem>
                        </ComboBox>

                        <Separator/>
                        <Label Content="Anti-AFK (Multiplayer):" HorizontalAlignment="Center"/>
                        <ComboBox Width="150" HorizontalContentAlignment="Center" Padding="10" x:Name="anti_afk" SelectedValuePath="Tag" SelectionChanged="AntiAFK_SelectionChanged" Margin="0,0,0,20">
                            <ComboBoxItem Tag="An">An</ComboBoxItem>
                            <ComboBoxItem Tag="Aus">Aus</ComboBoxItem>
                        </ComboBox>


                        <TextBox x:Name="anti_ak_text" Padding="5" Margin="0,0,0,0" Height="50" Width="250" TextWrapping="Wrap"/>
                        <Label x:Name="laenge_antiafk_text" FontSize="10" HorizontalAlignment="Center"/>

                        <Slider x:Name="antiafk_zeit" Width="200" Minimum="2" Maximum="4" TickFrequency="1" IsSnapToTickEnabled="True" TickPlacement="Both"/>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Label Content="{Binding ElementName=antiafk_zeit,Path=Value}" />
                            <Label Content=" Min."/>
                        </StackPanel>

                        <Button x:Name="anti_afk_text_save" Width="150" Height="40" Click="anti_afk_text_save_Click" Content="Anti-AFK speichern" Margin="0,0,0,10" Background="{DynamicResource MahApps.Brushes.AccentBase}"/>
                        <Separator/>

                        <Label Content="Hintergrund" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Label Content="Hintergrundbild:" HorizontalAlignment="Center" FontSize="14"/>
                        <ComboBox Width="150" HorizontalContentAlignment="Center" Padding="10" x:Name="Background_WEchsler" SelectedValuePath="Tag" SelectionChanged="Background_SelectionChanged" Margin="0,0,0,20">
                            <ComboBoxItem Tag="pj_1.jpg">Janus 1 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_2.jpg">Janus 2 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_3.jpg">Janus 3 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_4.jpg">Janus 4 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_5.jpg">Janus 5 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_6.jpg">Janus 6 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_7.jpg">Janus 7 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="pj_8.jpg">Janus 8 (Neu)</ComboBoxItem>
                            <ComboBoxItem Tag="div_1.jpg">Divers 1</ComboBoxItem>
                            <ComboBoxItem Tag="div_2.jpg">Divers 2</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_1.jpg">LKW 1</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_2.jpg">LKW 2</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_3.jpg">LKW 3</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_4.jpg">LKW 4</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_5.jpg">LKW 5</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_6.jpg">LKW 6</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_7.jpg">LKW 7</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_8.jpg">LKW 8</ComboBoxItem>
                            <ComboBoxItem Tag="lkw_9.jpg">LKW 9</ComboBoxItem>
                            <ComboBoxItem Tag="woman_1.jpg">Frau 1</ComboBoxItem>
                            <ComboBoxItem Tag="woman_2.jpg">Frau 2</ComboBoxItem>
                            <ComboBoxItem Tag="woman_3.jpg">Frau 3</ComboBoxItem>
                            <ComboBoxItem Tag="woman_4.jpg">Frau 4</ComboBoxItem>
                            <ComboBoxItem Tag="woman_5.jpg">Frau 5</ComboBoxItem>
                            <ComboBoxItem Tag="woman_6.jpg">Frau 6</ComboBoxItem>
                            <ComboBoxItem Tag="woman_7.jpg">Frau 7</ComboBoxItem>
                            <ComboBoxItem Tag="patti_1.jpg">Patti 1</ComboBoxItem>
                        </ComboBox>
                        <Label Content="oder:" HorizontalAlignment="Center" Margin="0,-20" FontSize="14"/>
                        <Button x:Name="eigene_bg_bild" Click="OPEN_BG_FILE" Content="Eigenes Hintergrundbild" Margin="0,10,0,20" Width="150" Height="40" Background="{DynamicResource MahApps.Brushes.AccentBase}"/>

                        <Label HorizontalAlignment="Center" Content="Background-Transparenz:"/>
                        <ComboBox Width="150" HorizontalContentAlignment="Center" Padding="10" x:Name="bg_opacity" SelectedValuePath="Tag" SelectionChanged="bg_opacity_ValueChanged" Margin="0,0,0,20">
                            <ComboBoxItem Tag="1.0">100 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.9">90 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.8">80 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.7">70 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.6">60 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.5">50 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.4">40 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.3">30 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.2">20 %</ComboBoxItem>
                            <ComboBoxItem Tag="0.1">10 %</ComboBoxItem>
                        </ComboBox>


                        <Button Content="LOG-Ordner öffnen" Click="LOG_ORDNER_OEFFNEN" Width="150" Height="30" Background="{DynamicResource MahApps.Brushes.Accent}"/>

                        <Separator Margin="0,20,0,20"/>


                        <StackPanel Orientation="Horizontal">
                            <Label Content="ETS2-Pfad:" Margin="0,0,20,0"/>
                            <Label x:Name="ets2_content"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="ATS-Pfad:" Margin="0,0,20,0"/>
                            <Label x:Name="ats_content"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label Content="TMP-Pfad:" Margin="0,0,20,0"/>
                            <Label x:Name="tmp_content"/>
                        </StackPanel>

                        <Button Content="Spielpfade ändern..." Click="spielpfade_aendern" Width="150" Height="30" Background="{DynamicResource MahApps.Brushes.Accent}"/>


                        <Separator Margin="0,20,0,20"/>
                        <Label Content="Werbepartner:" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button Width="230" Height="70" Background="{DynamicResource MahApps.Brushes.Accent}" Click="Oeffne_Andras_TV" Margin="0,0,0,10">
                            <StackPanel Orientation="Horizontal">
                                <Image Width="230" HorizontalAlignment="Center" Source="Images\Panel_Werbung.jpg"/>
                            </StackPanel>
                        </Button>


                        <Separator/>
                        <Label Content="Spenden:" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button Width="230" Height="70" Background="{DynamicResource MahApps.Brushes.Accent}" Click="Spende_einen_Kaffee">
                            <StackPanel Orientation="Horizontal">
                                <Image Height="50" HorizontalAlignment="Left" Source="{iconPacks:BoxIconsImage Kind=LogosPaypal}"/>
                                <TextBlock Width="160" TextWrapping="Wrap" FontSize="14" VerticalAlignment="Center" Margin="10,0,0,0">Spende uns einen Kaffee</TextBlock>
                            </StackPanel>
                        </Button>

                        <Button Width="230" Height="70" Background="{DynamicResource MahApps.Brushes.Accent}" Click="patreon_link" Margin="0,10,0,0">
                            <StackPanel Orientation="Horizontal">
                                <Image Height="50" HorizontalAlignment="Left" Source="{iconPacks:BoxIconsImage Kind=LogosPatreon}"/>
                                <TextBlock Width="160" FontSize="14" VerticalAlignment="Center" Margin="10,0,0,0" TextWrapping="Wrap">Werde Unterstützer des Projekts</TextBlock>
                            </StackPanel>
                        </Button>
                        
                        <Separator/>
                        <Label Content="Suche Client Updates" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button x:Name="Client_Updates" Click="Client_Update_Manuell" Content="Suche manuell nach Updates" HorizontalAlignment="Center" Width="230" Height="30" Background="Red" Foreground="White"/>

                        <Separator/>
                        <Label Content="Client-Reset" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button x:Name="Werksteinstellungen" Click="Werksteinstellungen_Click" Content="Client RESET!" HorizontalAlignment="Center" Width="230" Height="30" Background="Red" Foreground="White"/>

                        <Separator/>
                        <Label Content="ETS-Tour löschen" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button x:Name="ETS_TOUR_delete" Click="ets_tour_delete" Content="ETS Tour löschen" HorizontalAlignment="Center" Width="230" Height="30" Background="Red" Foreground="White"/>
                       
                        <Separator/>
                        <Label Content="ATS-Tour löschen" FontSize="24" HorizontalAlignment="Center" Foreground="{DynamicResource MahApps.Brushes.Accent}"/>
                        <Button x:Name="ATS_TOUR_delete" Click="ats_tour_delete" Content="ATS Tour löschen" HorizontalAlignment="Center" Width="230" Height="30" Background="Red" Foreground="White"/>


                    </StackPanel>
                </ScrollViewer>
            </mah:Flyout>

            <mah:Flyout x:Name="RightFlyOut" Width="600" Position="Right" Foreground="White">
           
                <StackPanel Orientation="Vertical">
                    <Label FontSize="20" x:Name="credit_text" Margin="30,0,0,0"/>
                    <Button Width="300" Click="Andras_Click">
                        <Button.Content>
                            <StackPanel Orientation="Vertical">
                                <Image Source="Images/Panel_Werbung.jpg" Cursor="Hand"/>
                            </StackPanel>
                        </Button.Content>
                    </Button>
                </StackPanel>

            </mah:Flyout>

            <mah:Flyout x:Name="UpdatesFlyOut" Width="600" Position="Right" Foreground="White">

                <ScrollViewer Width="600" VerticalScrollBarVisibility="Auto">
                    <TextBlock x:Name="update_text" Margin="20,0,0,0" FontSize="14" TextWrapping="Wrap"/>
                </ScrollViewer>

            </mah:Flyout>



            <mah:Flyout x:Name="BottomFlyOut" Height="180" Position="Bottom" Background="Orange" Foreground="Black">

                <StackPanel Orientation="Vertical">
                    <Label Content="Bitte gib das Land und den Tankpreis ein !" Foreground="Black" FontSize="26"/>
                    <Separator/>

                    <StackPanel Orientation="Horizontal">
                        <ComboBox x:Name="eingabe_land" Height="40" Width="200" FontSize="18" SelectedValue="Tag">
                            <ComboBoxItem Tag="Belgien">Belgien</ComboBoxItem>
                            <ComboBoxItem Tag="Bulgarien">Bulgarien</ComboBoxItem>
                            <ComboBoxItem Tag="Deutschland" IsSelected="True">Deutschland</ComboBoxItem>
                            <ComboBoxItem Tag="Dänemark">Dänemark</ComboBoxItem>
                            <ComboBoxItem Tag="Estland">Estland</ComboBoxItem>
                            <ComboBoxItem Tag="Frankreich">Frankreich</ComboBoxItem>
                            <ComboBoxItem Tag="Finnland">Finnland</ComboBoxItem>
                            <ComboBoxItem Tag="Großbritannien">Großbritannien</ComboBoxItem>
                            <ComboBoxItem Tag="Italien">Italien</ComboBoxItem>
                            <ComboBoxItem Tag="Lettland">Lettland</ComboBoxItem>
                            <ComboBoxItem Tag="Litauen">Litauen</ComboBoxItem>
                            <ComboBoxItem Tag="Luxemburg">Luxemburg</ComboBoxItem>
                            <ComboBoxItem Tag="Niederland">Niederlande</ComboBoxItem>
                            <ComboBoxItem Tag="Österreich">Österreich</ComboBoxItem>
                            <ComboBoxItem Tag="Polen">Polen</ComboBoxItem>
                            <ComboBoxItem Tag="Portugal">Portugal</ComboBoxItem>
                            <ComboBoxItem Tag="Rumänien">Rumänien</ComboBoxItem>
                            <ComboBoxItem Tag="Russland">Russland</ComboBoxItem>
                            <ComboBoxItem Tag="Schweden">Schweden</ComboBoxItem>
                            <ComboBoxItem Tag="Schweiz">Schweiz</ComboBoxItem>
                            <ComboBoxItem Tag="Slowakei">Slowakai</ComboBoxItem>
                            <ComboBoxItem Tag="Spanien">Spanien</ComboBoxItem>
                            <ComboBoxItem Tag="Tschechien">Tschechien</ComboBoxItem>
                            <ComboBoxItem Tag="Türkei">Türkei</ComboBoxItem>
                            <ComboBoxItem Tag="Ungarn">Ungarn</ComboBoxItem>
                        </ComboBox>

                        <Label Content="{Binding EURO_DOLLAR}" FontSize="30" Foreground="Black" VerticalAlignment="Center" HorizontalContentAlignment="Right" HorizontalAlignment="Center" Margin="20,0,0,0" />
                        <mah:NumericUpDown x:Name="preisangabe" Width="150" Value="1.24" InterceptManualEnter="False" Maximum="1.99" Minimum="1.00" TextAlignment="Center" InterceptMouseWheel="False" InterceptArrowKeys="True" Interval="0.01" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="24" Height="40" Margin="10,0,0,0"/>


                    </StackPanel>
                    <Button x:Name="preis_eintragen" Height="40" Width="407" Content="Preis je Liter eintragen:" Background="Green" HorizontalAlignment="Left" Margin="0,20,0,0" Padding="10" Click="Preis_eintragen_Click"/>

                </StackPanel>



            </mah:Flyout>

        </mah:FlyoutsControl>



    </mah:MetroWindow.Flyouts>


    <mah:MetroWindow.LeftWindowCommands>
        <mah:WindowCommands>

            <ToggleButton Content="Layers" ToolTip="Hauptmenü" Click="Hauptmenu_Click" IsChecked="{Binding ElementName=LeftFlyOut, Path=IsOpen}" Cursor="Hand">
                <ToggleButton.ContentTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <iconPacks:PackIconFontAwesome Kind="BarsSolid" Width="100" Height="25" />
                        </StackPanel>
                    </DataTemplate>
                </ToggleButton.ContentTemplate>
            </ToggleButton>

            <ToggleButton Content="Layers" ToolTip="CREDITS" Click="Oeffne_Credits" IsChecked="{Binding ElementName=RightFlyOut, Path=IsOpen}" Cursor="Hand">
                <ToggleButton.ContentTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <iconPacks:PackIconFontAwesome Kind="HandsHelpingSolid" Width="100" Height="30" />
                        </StackPanel>
                    </DataTemplate>
                </ToggleButton.ContentTemplate>
            </ToggleButton>

        </mah:WindowCommands>
    </mah:MetroWindow.LeftWindowCommands>

    <mah:MetroWindow.RightWindowCommands>
        <mah:WindowCommands>

            <Label x:Name="user_online_anzeige" Content="{Binding ONLINEUSER}" FontSize="16" Margin="0,7,0,0"/>

            <ToggleButton Content="Layers" ToolTip="Zeige Client-Updates" IsChecked="{Binding ElementName=UpdatesFlyOut, Path=IsOpen}" Cursor="Hand">
                <ToggleButton.ContentTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <iconPacks:PackIconModern Kind="VisualstudioNews" Width="25" Height="25" />
                        </StackPanel>
                    </DataTemplate>
                </ToggleButton.ContentTemplate>
            </ToggleButton>



            <!-- DISABLED WEIL DIE TELE AUS GEHT IM MINIMIZE
            <Button Click="Minimize_Window" ToolTip="Minimiere das Fenster">
                <iconPacks:PackIconModern Width="22" Height="22" Kind="WindowMinimize" />
            </Button>
            -->

        </mah:WindowCommands>
    </mah:MetroWindow.RightWindowCommands>



    <DockPanel>

        <StatusBar DockPanel.Dock="Bottom" Height="30" FontSize="12" HorizontalAlignment="Stretch">
            <StatusBarItem>
                <Label Content="{Binding TELEMETRY_VERSION}" Foreground="Black"/>
            </StatusBarItem>
            <StatusBarItem>
                <Label Content="{Binding DLL_VERSION}" Foreground="Black"/>
            </StatusBarItem>
            <StatusBarItem>
                <StackPanel Orientation="Horizontal">
                    <Image Source="{iconPacks:BoxIconsImage Kind=LogosPatreon}" Height="20" ToolTip="Dein Patreon-Level" VerticalAlignment="Bottom"/>
                    <Label Content="{Binding PATREON_LEVEL}" VerticalAlignment="Center" Foreground="Black"/>
                </StackPanel>

            </StatusBarItem>

            <StatusBarItem HorizontalAlignment="Right">
                <StackPanel Orientation="Horizontal">
                    <StatusBarItem x:Name="status_bar_version" Foreground="Black"/>
                </StackPanel>
            </StatusBarItem>
        </StatusBar>

        <Grid x:Name="GRID_FAHRT">
            <Grid.RowDefinitions>
                <RowDefinition Height="13"/>
                <RowDefinition Height="127"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="70"/>
                <RowDefinition Height="10"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="10"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>

                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="135"/>
                <ColumnDefinition Width="55"/>
                <ColumnDefinition Width="60"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="59"/>
                <ColumnDefinition Width="141"/>
            </Grid.ColumnDefinitions>


            <!-- WAITING ZONE -->

            <Rectangle x:Name="Waiting_Rect" Fill="{DynamicResource MahApps.Brushes.AccentBase}" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="20" Grid.RowSpan="20" Panel.ZIndex="1000" Visibility="{Binding SDK_AKTIVE, Converter={StaticResource SDK_Aktiv}}"/>
            <Label Content="Warte auf ETS / ATS" Grid.Row="3" Height="100" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="20" FontSize="72" Panel.ZIndex="1001" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Visibility="{Binding SDK_AKTIVE, Converter={StaticResource SDK_Aktiv}}"/>
            <mah:MetroProgressBar Height="100" Panel.ZIndex="1002" IsIndeterminate="True" EllipseDiameter="15" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="20" Grid.RowSpan="2" Visibility="{Binding SDK_AKTIVE, Converter={StaticResource SDK_Aktiv}}"/>
